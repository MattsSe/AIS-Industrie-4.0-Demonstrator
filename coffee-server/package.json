{
  "name": "coffee-server",
  "version": "0.0.0",
  "private": true,
  "main" : "index.js",
  "scripts": {
    "start": "node --require ts-node/register index.ts",
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc -w",
    "dev": "nodemon ./dist/index.js",
    "demon": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "clean": "rimraf dist",
    "docs": "compodoc -p tsconfig.json -d docs",
    "docs:serve": "compodoc -p tsconfig.json -d docs -s"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "codelyzer": "^4.4.4",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "http-errors": "~1.6.2",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "node-sass-middleware": "0.11.0",
    "pug": "2.0.0-beta11",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.1",
    "sequelize-typescript": "^0.6.7-beta.0",
    "sqlite3": "^4.0.2",
    "strong-error-handler": "^3.2.0"
  },
  "devDependencies": {
    "compodoc": "0.0.41",
    "concurrently": "^4.0.1",
    "@types/sqlite3": "^3.1.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.10.1",
    "nodemon": "^1.18.4",
    "karma": "^3.0.0",
    "rimraf": "^2.6.2",
    "npm-run-all": "4.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.0.3"
  }
}
